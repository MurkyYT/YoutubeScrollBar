{
    "manifest_version": 3,
    "name": "Youtube Horizontal Scrollbar Hider",
    "description": "Youtube Horizontal Scrollbar Hider",
    "homepage_url": "https://github.com/MurkyYT/YoutubeScrollBar",
    "version": "0.0.1",
    "icons": {},
    "action": {
        "default_title": "Youtube Horizontal Scrollbar Hider"
    },
    "permissions": [],
    "host_permissions": [
        "*://www.youtube.com/*"
    ],
    "content_scripts": [
        {
            "js": [
                "foreground.js"
            ],
            "matches": [
                "https://www.youtube.com/watch?v=*"
            ]
        }
    ]
}